{
  "manifest_version": 2,
  "name": "Resizing Extension",
  "version": "1.0.0",
  "description": "A browser extension for resizing windows.",
  "icons": {
    "16": "icon-16.png",
    "19": "icon-19.png",
    "24": "icon-24.png",
    "32": "icon-32.png",
    "36": "icon-36.png",
    "38": "icon-38.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "19": "icon-19.png",
      "24": "icon-24.png",
      "32": "icon-32.png",
      "36": "icon-36.png",
      "38": "icon-38.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    },
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "system.display",
    "webNavigation"
  ]
}